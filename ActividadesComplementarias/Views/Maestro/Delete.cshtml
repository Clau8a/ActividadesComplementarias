@model ActividadesComplementarias.Models.Maestros

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-4 col-lg-offset-4">
    <h2>Delete</h2>

    <h3>¿Seguro que desea eliminar esto?</h3>

    <div class="display-label">
        <label>Clave Maestro</label>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.idMaestro)
    </div>

    <div class="display-label">
        <label>Nombre Maestro</label>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.nombreMaestro)
    </div>

    <div class="display-label">
        <label>Departamento</label>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Departamento.nombreDepartamento)
    </div>

    <div class="display-label">
        <label>Tipo</label>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.TipoMaestro1.tipoMaestro1)
    </div>

    <div class="display-label">
        <label>Contraseña</label>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.contraseñaMaestro)
    </div>

    <div class="display-label">
        <label>Salt Contraseña</label>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.saltContraseña)
    </div>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="botones">
            <input class="btn-success" type="submit" value="Eliminar" />
        </div>
    }
</div>