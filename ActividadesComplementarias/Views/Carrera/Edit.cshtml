@model ActividadesComplementarias.Models.Carrera

@{
    ViewBag.Title = "CAS - Editar Carrera";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Editar Carrera</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

        <div class="editor-label">
            <label>ID Carrea</label>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.idCarrera)
            @Html.ValidationMessageFor(model => model.idCarrera)
        </div>

        <div class="editor-label">
            <label>Nombre de Carrera</label>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nombreCarrera)
            @Html.ValidationMessageFor(model => model.nombreCarrera)
        </div>

        <p>
            <input type="submit" value="Guardar" />
        </p>
}

<div>
    @Html.ActionLink("Regresar a lista de Carreras", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
